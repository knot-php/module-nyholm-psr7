{
  "name": "knot-php/module-nyholm-psr7",
  "description": "nyholm/psr7 installer",
  "keywords": ["PHP","PSR-7","Module","Installer","kNot Framework"],
  "homepage": "https://github.com/knot-php/module-nyholm-psr7",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "psr/http-message": "^1.0",
    "nyholm/psr7": "^1.4",
    "nyholm/psr7-server": "^1.0",
    "knot-lib/kernel": "~0.1",
    "knot-lib/module": "~0.1",
    "knot-lib/support": "~0.1",
    "knot-php/module-knot-pipeline": "~0.1",
    "knot-php/module-stk2k-eventstream": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "knotphp\\module\\nyholmpsr7\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "knotphp\\module\\nyholmsr7\\test\\": "test/"
    }
  }
}
